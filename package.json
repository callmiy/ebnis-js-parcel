{
  "name": "@kanmii/parcel-react-template",
  "version": "0.0.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "env-cmd -e dev parcel src/index.html",
    "build": "rimraf dist && env-cmd -e prod parcel build dist/index.html --public-url .",
