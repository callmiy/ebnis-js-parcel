{
  "name": "ebnis",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -e dev yarn react-scripts start",
    "build-prod": "env-cmd -e REACT_APP_API_prod  react-scripts build",
    "build-local-staging": "env-cmd -e local-staging react-scripts build",
    "test": "env-cmd -e test yarn react-scripts test",
    "test:coverage": "yarn test --coverage",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@absinthe/socket": "0.2.1",
    "@absinthe/socket-apollo-link": "0.2.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-cache-persist": "0.1.1",
    "apollo-client": "2.5.1",
    "apollo-link-error": "1.1.10",
    "apollo-link-http": "1.5.14",
    "apollo-link-state": "0.4.2",
    "date-fns": "1.29.0",
    "formik": "1.5.4",
    "graphql": "14.2.1",
    "graphql-tag": "2.10.1",
    "lodash-es": "4.17.11",
    "phoenix": "1.4.3",
    "react": "16.9.0-alpha.0",
    "react-apollo": "2.5.4",
    "react-dom": "16.9.0-alpha.0",
    "react-router-dom": "4.3.1",
    "react-scripts": "3.0.0",
    "semantic-ui-css-offline": "1.0.3-2.4.1",
    "semantic-ui-react": "0.86.0",
    "yup": "0.27.0"
  },
  "devDependencies": {
    "@types/graphql": "14.2.0",
    "@types/jest": "24.0.12",
    "@types/lodash-es": "4.17.3",
    "@types/node": "12.0.0",
    "@types/phoenix": "1.4.0",
    "@types/react": "16.8.17",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.1",
    "@types/yup": "0.26.13",
    "cross-env": "5.2.0",
    "env-cmd": "9.0.1",
    "jest-dom": "3.1.3",
    "node-sass": "4.11.0",
    "react-testing-library": "6.1.2",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-react": "4.0.0",
    "typescript": "3.4.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/Home/index.ts",
      "!src/**/NewExp/index.ts"
    ]
  },
  "resolutions": {
    "babel-jest": "24.7.1"
  }
}
